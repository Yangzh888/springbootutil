<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.springbootutil.dao.UserMapper">

    <select id="getUser" resultType="com.cn.springbootutil.entity.User" >
        select * from sys_user where id='1'
    </select>

    <select id="selectPageVo" resultType="com.cn.springbootutil.entity.User">
    SELECT id,username FROM sys_user
</select>
</mapper>
